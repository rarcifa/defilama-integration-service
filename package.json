{
  "name": "cd-details-service",
  "version": "1.0.0",
  "description": "cronos dapp details service endpoint for the cronos dapp portal",
  "main": "server.ts",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "ts-node -r tsconfig-paths/register ./src/server.ts",
    "start": "node ./dist/server.js",
    "prod": "npm run build && npm run start",
    "pretty": "prettier --write \"./**/*.{ts,js,json}\"",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "author": "rarcifa",
  "license": "ISC",
  "dependencies": {
    "@types/express": "4.17.15",
    "amqplib": "0.10.3",
    "axios": "0.25.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-validator": "6.14.2",
    "jsonwebtoken": "9.0.0",
    "module-alias": "2.2.2",
    "mongoose": "6.8.1",
    "node-cron": "3.0.2",
    "nodemon": "2.0.20",
    "randombytes": "2.1.0",
    "swagger-jsdoc": "6.2.7",
    "swagger-ui-express": "4.6.0",
    "ts-jest": "29.0.5",
    "tsc-alias": "1.8.2",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.6",
    "@types/amqplib": "^0.10.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node-cron": "^3.0.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "jest": "^29.4.1",
    "prettier": "^2.5.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  }
}
